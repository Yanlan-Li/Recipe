<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="365dp"
        android:layout_height="400dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="热门搜索"
            android:textColor="#333333"
            android:textSize="20sp"
            />
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tangcu"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:text="糖醋排骨"/>

            <TextView
                android:id="@+id/kele"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="可乐鸡翅"/>
            <TextView
                android:id="@+id/qiezi"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="茄子"/>
            <TextView
                android:id="@+id/tudou"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="土豆"/>

        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/hongshaorou"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:text="红烧肉"/>
            <TextView
                android:id="@+id/jichi"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="鸡蛋"/>
            <TextView
                android:id="@+id/paigu"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="排骨"/>
            <TextView
                android:id="@+id/hongshaoqiezi"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="红烧茄子"/>

        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/doufu"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:text="豆腐"/>
            <TextView
                android:id="@+id/jirou"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="鸡肉"/>
            <TextView
                android:id="@+id/yuecai"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="粤菜"/>
            <TextView
                android:id="@+id/niurou"
                android:paddingVertical="10dp"
                android:paddingHorizontal="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="16sp"
                android:background="#F5F5F5"
                android:layout_marginLeft="15dp"
                android:text="牛肉"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

</layout>